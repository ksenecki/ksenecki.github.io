{
  "name": "pandas-story",
  "version": "1.0.0",
  "description": "This is Panda story",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint .",
    "test": "gulp test",
    "start": "gulp serve",
    "build": "gulp build",
    "preinstall": "npx npm-force-resolutions"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown --no-stash"
  },
  "resolutions": {
    "glob-parent": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksenecki/ksenecki.github.io.git"
  },
  "author": "Kamil Senecki",
  "license": "",
  "bugs": {
    "url": "https://github.com/ksenecki/ksenecki.github.io/issues"
  },
  "homepage": "",
  "dependencies": {
    "prettier": "^2.7.1",
    "reveal.js": "^4.3.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "typescript": "^4.8.4",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "colors": "^1.4.0",
    "core-js": "^3.12.1",
    "fitty": "^2.3.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-connect": "^5.7.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-tap": "^2.0.0",
    "gulp-zip": "^4.2.0",
    "highlight.js": "^10.0.3",
    "marked": "^4.0.12",
    "node-qunit-puppeteer": "^2.1.0",
    "qunit": "^2.17.2",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.39.2",
    "yargs": "^15.1.0"
  },
  "subdomain": "",
  "module": "",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "reveal",
    "slides",
    "presentation"
  ],
  "browserslist": "> 2%, not dead",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "globals": {
      "module": false,
      "console": false,
      "unescape": false,
      "define": false,
      "exports": false
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "new-cap": 2,
      "no-caller": 2,
      "dot-notation": 0,
      "no-eq-null": 2,
      "no-unused-expressions": 0
    }
  }
}
